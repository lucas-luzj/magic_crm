<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:flowable="http://flowable.org/bpmn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="bpmn-js" exporterVersion="17.11.1">
  <bpmn:process id="leave_application_process" name="请假申请流程" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="开始">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:userTask id="leave_application_task" name="请假申请" flowable:formKey="leave_application">
      <bpmn:extensionElements>
        <flowable:formProperty id="leaveType" name="请假类型" type="enum" required="true">
          <flowable:value id="annual" name="年假" />
          <flowable:value id="sick" name="病假" />
          <flowable:value id="personal" name="事假" />
          <flowable:value id="compensatory" name="调休" />
          <flowable:value id="maternity" name="产假" />
          <flowable:value id="paternity" name="陪产假" />
        </flowable:formProperty>
        <flowable:formProperty id="startDate" name="开始日期" type="date" required="true" />
        <flowable:formProperty id="endDate" name="结束日期" type="date" required="true" />
        <flowable:formProperty id="leaveDays" name="请假天数" type="long" required="true" />
        <flowable:formProperty id="reason" name="请假理由" type="string" required="true" />
        <flowable:formProperty id="emergencyContact" name="紧急联系人" type="string" />
        <flowable:formProperty id="attachment" name="相关证明" type="string" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:exclusiveGateway id="Gateway_1" name="请假天数判断">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:userTask id="manager_approval_task" name="经理审批" flowable:formKey="manager_approval" flowable:assignee="${initiator}">
      <bpmn:extensionElements>
        <flowable:formProperty id="leaveType" name="请假类型" type="string" readable="true" writable="false" />
        <flowable:formProperty id="startDate" name="开始日期" type="date" readable="true" writable="false" />
        <flowable:formProperty id="endDate" name="结束日期" type="date" readable="true" writable="false" />
        <flowable:formProperty id="leaveDays" name="请假天数" type="long" readable="true" writable="false" />
        <flowable:formProperty id="reason" name="请假理由" type="string" readable="true" writable="false" />
        <flowable:formProperty id="managerApproval" name="审批意见" type="enum" required="true">
          <flowable:value id="approve" name="同意" />
          <flowable:value id="reject" name="拒绝" />
        </flowable:formProperty>
        <flowable:formProperty id="managerComment" name="审批备注" type="string" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:exclusiveGateway id="Gateway_2" name="经理审批结果">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:userTask id="hr_review_task" name="HR复核" flowable:formKey="hr_review" flowable:candidateGroups="hr">
      <bpmn:extensionElements>
        <flowable:formProperty id="leaveType" name="请假类型" type="string" readable="true" writable="false" />
        <flowable:formProperty id="startDate" name="开始日期" type="date" readable="true" writable="false" />
        <flowable:formProperty id="endDate" name="结束日期" type="date" readable="true" writable="false" />
        <flowable:formProperty id="leaveDays" name="请假天数" type="long" readable="true" writable="false" />
        <flowable:formProperty id="reason" name="请假理由" type="string" readable="true" writable="false" />
        <flowable:formProperty id="managerApproval" name="经理审批意见" type="string" readable="true" writable="false" />
        <flowable:formProperty id="managerComment" name="经理审批备注" type="string" readable="true" writable="false" />
        <flowable:formProperty id="hrReview" name="HR复核意见" type="enum" required="true">
          <flowable:value id="approve" name="通过" />
          <flowable:value id="reject" name="驳回" />
        </flowable:formProperty>
        <flowable:formProperty id="hrComment" name="HR复核备注" type="string" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:exclusiveGateway id="Gateway_3" name="HR复核结果">
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:endEvent id="EndEvent_Approved" name="请假通过">
      <bpmn:incoming>Flow_9</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:endEvent id="EndEvent_Rejected" name="请假被拒">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:incoming>Flow_10</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- 流程连线 -->
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="leave_application_task" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="leave_application_task" targetRef="Gateway_1" />
    <bpmn:sequenceFlow id="Flow_3" name="≤3天" sourceRef="Gateway_1" targetRef="manager_approval_task">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${leaveDays &lt;= 3}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_4" name=">3天" sourceRef="Gateway_1" targetRef="manager_approval_task">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${leaveDays &gt; 3}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_5" sourceRef="manager_approval_task" targetRef="Gateway_2" />
    <bpmn:sequenceFlow id="Flow_6" name="同意" sourceRef="Gateway_2" targetRef="hr_review_task">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${managerApproval == 'approve'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_7" name="拒绝" sourceRef="Gateway_2" targetRef="EndEvent_Rejected">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${managerApproval == 'reject'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_8" sourceRef="hr_review_task" targetRef="Gateway_3" />
    <bpmn:sequenceFlow id="Flow_9" name="通过" sourceRef="Gateway_3" targetRef="EndEvent_Approved">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${hrReview == 'approve'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_10" name="驳回" sourceRef="Gateway_3" targetRef="EndEvent_Rejected">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${hrReview == 'reject'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
  </bpmn:process>
  
  <!-- BPMN图形信息 -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="leave_application_process">
      <bpmndi:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
