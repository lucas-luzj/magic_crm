      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="manager_approval_task_di" bpmnElement="manager_approval_task">
        <dc:Bounds x="500" y="80" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Gateway_2_di" bpmnElement="Gateway_2" isMarkerVisible="true">
